{
  "version": "0.0.0",
  "name": "memo-ui",
  "files": [
    "dist"
  ],
  "description": "An efficient component library built on vue3、ts、vite",
  "keywords": [
    "vue3",
    "ui",
    "vue"
  ],
  "main": "./dist/memo-ui.umd.js",
  "module": "./dist/memo-ui.es.js",
  "types": "./src/index.ts",
  "homepage": "https://github.com/Mrxyy/memo-ui",
  "bugs": {
    "url": "https://github.com/Mrxyy/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mrxyy/memo-ui"
  },
  "exports": {
    ".": {
      "import": "./dist/memo-ui.es.js",
      "require": "./dist/memo-ui.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "noEmit": "vue-tsc --noEmit",
    "lint:js": "eslint --ext \".js,.ts\" --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "prepare": "husky install",
    "watch": "npm-watch",
    "commitlint": "npx --no-install commitlint --edit $1"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "*",
      "quiet": false
    }
  },
  "engines": {
    "node": "15.x"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.6",
    "@vueuse/head": "^0.6.0",
    "bootstrap-icons": "^1.5.0",
    "codemirror": "^5.62.0",
    "dedent": "^0.7.0",
    "highlight.js": "^11.1.0",
    "lint-staged": "^11.1.1",
    "postcss-scss": "^4.0.0",
    "quill": "^1.3.7",
    "vue": "^3.0.5",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/node": "^16.3.1",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.0.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^7.0.1",
    "npm-watch": "^0.11.0",
    "sass": "^1.35.2",
    "serve-static": "^1.14.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.2",
    "vite": "^2.4.2",
    "vite-plugin-md": "^0.8.0",
    "vue-tsc": "^0.0.24"
  },
  "lint-staged": {
    "*.s?css": "npm run lint:style",
    "*.vue": "npm run lint:style"
  }
}